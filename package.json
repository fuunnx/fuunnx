{
  "name": "geraudhenrion.com",
  "version": "0.0.0",
  "description": "Géraud Henrion's portfolio",
  "scripts": {
    "start": "node .scripts/renderStaticPages && webpack-dev-server --config ./webpack.dev.js --define  process.env.NODE_ENV='\"development\"' --define  process.env.RUN_CONTEXT='\"browser\"' --inline --hot --progress --watch",
    "build": "rimraf ./dist/ && node .scripts/renderStaticPages && cp ./.scripts/temp/styles.css ./dist/ && webpack --config ./webpack.prod.js --define process.env.NODE_ENV='\"production\"' --define  process.env.RUN_CONTEXT='\"browser\"' -p --progress",
    "deploy": "gh-pages-deploy"
  },
  "author": "Géraud Henrion",
  "license": "ISC",
  "gh-pages-deploy": {
    "staticpath": "dist",
    "cname": "geraudhenrion.io",
    "prep": [
      "build"
    ],
    "noprompt": false
  },
  "devDependencies": {
    "@cycle/xstream-run": "^3.1.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-webpack-alias": "^1.3.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "^0.25.0",
    "cycle-restart": "0.0.14",
    "eslint": "^1.0.0",
    "eslint-config-cycle": "^3.0.0",
    "eslint-plugin-cycle": "^1.0.1",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-no-class": "^0.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gh-pages-deploy": "^0.4.2",
    "glob-watcher": "^3.0.0",
    "image-webpack-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "node-inspector": "^0.12.8",
    "nodemon": "^1.9.2",
    "performance-now": "^0.2.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.8.2",
    "progress-bar-webpack-plugin": "^1.9.0",
    "proxy-middleware": "^0.15.0",
    "rimraf": "^2.5.4",
    "static-site-generator-webpack-plugin": "^3.1.0",
    "style-loader": "^0.13.1",
    "supervisor": "^0.9.1",
    "svg-inline-loader": "^0.7.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "@cycle/dom": "^12.2.5",
    "@cycle/history": "^4.0.1",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "cycle-animation-driver": "^0.3.0",
    "cycle-canvas": "^0.3.0",
    "document-register-element": "^1.3.0",
    "fs-promise": "^0.5.0",
    "glob-promise": "^1.0.6",
    "history": "^3.2.1",
    "insertion-query": "^1.0.3",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.8.3",
    "markdown-string": "^0.2.0",
    "minimatch": "^3.0.3",
    "mkdirp": "^0.5.1",
    "normalize.css": "^4.2.0",
    "ramda": "^0.22.1",
    "snabbdom-virtualize": "^0.6.0",
    "xstream": "^8.0.0"
  }
}
