{
  "presets": [
    ["es2015",{
      "targets": {
        "browsers": ["last 2 versions"],
        "modules": false,
        "loose": true
      }
    }],
    ["es2017"],
    ["stage-2"]
  ],
  "plugins": [
    [ "transform-runtime" ],
    [ "babel-plugin-webpack-alias", { "config": "./webpack.config.js" } ],
    [ "ramda" ]
  ],
  "compact": true
}
